<table class="forumline" width="100%" cellspacing="0" cellpadding="0" border="0">
<tr><td class="row-header" colspan="7"><span>{L_TOP_DOWNLOADS}</span></td></tr>
<tr>
	<th height="25" nowrap="nowrap">#</th>
	<th nowrap="nowrap">{L_FILENAME}</th>
	<th nowrap="nowrap">{L_FILECOMMENT}</th>
	<th nowrap="nowrap">{L_SIZE}</th>
	<th nowrap="nowrap">{L_DOWNLOADS}</th>
	<th nowrap="nowrap">{L_POST_TIME}</th>
	<th nowrap="nowrap">{L_POSTED_IN_TOPIC}</th>
</tr>
<!-- BEGIN attachrow -->
<tr>
	<td class="row1 row-center"><span class="gen">&nbsp;{attachrow.ROW_NUMBER}&nbsp;</span></td>
	<td class="row1 row-center"><span class="gen">{attachrow.VIEW_ATTACHMENT}</span></td>
	<td class="row1 row-center"><span class="gen">{attachrow.COMMENT}</span></td>
	<td class="row1 row-center" valign="middle"><span class="gen">{attachrow.SIZE}</b></span></td>
	<td class="row1 row-center" valign="middle"><span class="gen">{attachrow.DOWNLOAD_COUNT}</span></td>
	<td class="row1 row-center" valign="middle"><span class="gen">{attachrow.POST_TIME}</span></td>
	<td class="row1 row-center" valign="middle"><span class="gen">{attachrow.POST_TITLE}</span></td>
</tr>
<!-- END attachrow -->
</table>